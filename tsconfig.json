{
  "compilerOptions": {
    "target": "es2017",
    "module": "esnext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "lib": ["es2017", "dom", "dom.iterable"],
    "noEmit": true,
    "declaration": false,
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "allowJs": false,
    "types": ["vite/client"]
  },
  "include": ["**/*.ts", "**/*.tsx"]
}
